{
	"name": "com.webos.app.volume",
	"version": "1.0.0",
	"description": "webOS Enact Volume Control",
	"author": "LG SWP",
	"main": "src/index.js",
	"scripts": {
		"serve": "nodemon --watch .env --exec \"enact serve\"",
		"pack": "enact pack",
		"pack-p": "enact pack -p",
		"watch": "enact pack --watch",
		"clean": "enact clean",
		"lint": "enact lint --strict .",
		"license": "enact license",
		"test": "echo \"No test suite\" && exit 0",
		"test-watch": "enact test --watch"
	},
	"license": "Apache-2.0",
	"private": true,
	"repository": "https://github.com/enyojs/webos-auto-volume.git",
	"enact": {
		"theme": "agate"
	},
	"eslintConfig": {
		"extends": "enact/strict"
	},
	"eslintIgnore": [
		"node_modules/*",
		"build/*",
		"dist/*"
	],
	"dependencies": {
		"@enact/agate": "^2.0.0",
		"@enact/core": "^4.5.0",
		"@enact/i18n": "^4.5.0",
		"@enact/spotlight": "^4.5.0",
		"@enact/ui": "^4.5.0",
		"@enact/webos": "^4.5.0",
		"classnames": "^2.2.6",
		"ilib": "^14.4.0",
		"immer": "^3.2.0",
		"prop-types": "^15.6.2",
		"ramda": "^0.26.1",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"cross-env": "^5.2.1",
		"nodemon": "^1.19.2"
	}
}
